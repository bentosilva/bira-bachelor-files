<deployment xmlns="http://xml.apache.org/axis/wsdd/" xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">
    <service name="Robot" provider="java:RPC">
        <parameter name="className" value="org.usp.robotwsmapped.Robot"/>
        <parameter name="allowedMethods" value="getNews"/>
        <beanMapping qname="myNS:RSS" xmlns:myNS="urn:Robot" languageSpecificType="java:org.usp.news.RSS"/>
    </service>
</deployment>

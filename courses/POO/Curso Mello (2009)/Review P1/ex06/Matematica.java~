public class Matematica {

	public int soma(int a, int b) { 
		System.out.println("soma(int a, int b)");
		return a+b;
	}

	public double soma(double a, double b) { 
		System.out.println("soma(double a, double b)");
		return a+b;
	}

	public static void main(String args[]) {
		Matematica m = new Matematica();
		System.out.println(m.soma(1, 2));
		System.out.println(m.soma(1.2, 2.5));
	}
}
